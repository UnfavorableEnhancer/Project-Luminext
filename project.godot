; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Project Luminext"
config/description="An ultimate block-stacking puzzle game experience"
config/version="0.1.2"
run/main_scene="res://scenery/main.tscn"
config/quit_on_go_back=false
config/features=PackedStringArray("4.5")
run/flush_stdout_on_print.debug=false
boot_splash/bg_color=Color(0, 0, 0, 1)
boot_splash/show_image=false
boot_splash/fullsize=false
boot_splash/use_filter=false
config/icon="res://misc/icon_256.png"
config/windows_native_icon="res://misc/icon_a.ico"

[audio]

buses/default_bus_layout="res://misc/default_bus_layout.tres"
buses/channel_disable_time=0.5

[autoload]

Player="*res://scripts/singletones/profile.gd"
Data="*res://scripts/singletones/data.gd"
Console="*res://scripts/singletones/console.gd"
RankingManager="*res://scripts/singletones/ranking_manager.gd"
Debug="*res://scripts/singletones/debuger.gd"

[compression]

formats/zstd/long_distance_matching=true
formats/zstd/compression_level=5

[debug]

gdscript/warnings/untyped_declaration=1
gdscript/warnings/inferred_declaration=1
shapes/collision/shape_color=Color(0, 0.857143, 1, 0.42)

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/window_width_override=1280
window/size/window_height_override=720
window/stretch/mode="canvas_items"
window/per_pixel_transparency/allowed=true
window/vsync/vsync_mode=false
mouse_cursor/custom_image="res://misc/cursor.png"

[editor]

movie_writer/movie_file="D:/Work/LUMINES/Project-Luminext/misc/test.avi"
movie_writer/disable_vsync=true
naming/script_name_casing=2
scene_naming=1
convert_text_resources_to_binary_on_export=true

[file_customization]

folder_colors={
"res://addons/": "pink",
"res://bin/": "gray",
"res://fonts/": "blue",
"res://images/": "green",
"res://internal/": "yellow",
"res://menu/": "red",
"res://misc/": "gray",
"res://scenery/": "orange",
"res://scripts/": "teal",
"res://shaders/": "pink",
"res://third_party/": "gray",
"res://translations/": "gray"
}

[filesystem]

import/blender/enabled=false

[gui]

timers/tooltip_delay_sec=0.2

[importer_defaults]

mp3={
"loop": false
}
ogg_vorbis={
"loop": false
}

